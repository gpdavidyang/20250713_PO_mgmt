{
  "version": 2,
  "builds": [
    {
      "src": "dist/public/**",
      "use": "@vercel/static"
    },
    {
      "src": "api/[...all].js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/[...all].js"
    },
    {
      "src": "/assets/(.*)",
      "dest": "dist/public/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|svg|ico|woff|woff2))",
      "dest": "dist/public/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dist/public/index.html"
    }
  ]
}